@page
@model TorneoFutbol.App.Frontend.Pages.Arbitros.DetailsModel
@{
}
<h1>Detalles de los arbitros</h1>
<table class="table table-dark">
    <tr>
        <th scope="col">Nombre</th>
        <td>@Model.arbitro.Nombre</td>
    </tr>
    <tr>
        <th scope="col">Documento</th>
        <td>@Model.arbitro.Documento</td>
    </tr>
    <tr>
        <th scope="col">Numero de Telefono</th>
        <td>@Model.arbitro.NumeroTelefono</td>
    </tr>
    <tr>
        <th scope="col">Colegio</th>
        <td>@Model.arbitro.Colegio</td>
    </tr>
</table>
<a class="btn btn-primary" asp-page="./List">Volver</a>